package main;

public class Test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int s = 2;
		long a = 3;
		
	whatsItDo("WATCH");
	
	String x = "a";
	String y = "b";
	System.out.println(x+y);

	}
	public static void whatsItDo (String str) {
		int len = str.length();
		if (len > 1) {
			String temp = str.substring(0, len -1);
			whatsItDo(temp);
			System.out.println(temp);
		}
	}

}
