		//System.out.println("Hello World");
		
		int age = 19; 
		double iq = 120.0; 
		boolean isPersonAlive = true; 
		String name = "Jack"; 
		
		if (age > 18) {
			if (isPersonAlive == true) {
				System.out.println("You are smart");	
				}}else if (iq >= 110) {
					System.out.println("You are also smart");
				}else if (name == "Jack") {
					System.out.println("You are also smart");}
				else {
					System.out.println("You are probably stupid");
		 
		}
	}
